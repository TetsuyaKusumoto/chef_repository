{
  "environment": "staging",
  "authorization": {
        "sudo": {
            "groups": ["deploy"],
            "users": [ "deploy"],
            "passwordless": true,
            "sudoers_defaults": [
                "env_reset",
                "mail_badpass",
                "secure_path=\"/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin\""
            ]
        }
  },
  "run_list": [
    "role[base]",
    "role[web]",
    "role[db]",
  ],
  "automatic": {
    "ipaddress": "192.168.10.11",
    "hostname": "testhostname"
  },
  "network":{
    "hostname": "www.kusumoto-staging.xyz"
  },
  "hosts":{
    "hostname: "www.kusumoto-staging.xyz"
  }
}
